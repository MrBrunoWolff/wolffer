{
  "name": "wolffer",
  "version": "1.0.0",
  "description": "Add some wolffing to your JS",
  "main": "build/index.js",
  "scripts": {
    "build:watch": "watch 'npm run build' src",
    "build": "babel src -d build --ignore 'src/*.test.js'",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "prepublish": "npm run build",
    "release": "np"
  },
