  "repository": {
    "type": "git",
    "url": "git+https://github.com/d0cz/wolffer.git"
  },
  "keywords": [
    "wolff",
    "topdabalada"
  ],
  "author": "Bruno Wolff (twitter.com/d0cz)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/d0cz/wolffer/issues"
  },
  "homepage": "https://github.com/d0cz/wolffer#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-env": "^1.2.2",
    "np": "^2.13.1",
    "watch": "^1.0.2"
  }
}
